export OS_USERNAME=<%= @keystone_admin_user %>
<% if @keystone_service_token -%>
export OS_SERVICE_TOKEN='<%= @keystone_service_token.gsub(/'/){ %q(\') } %>'
<% end -%>
<% if @keystone_ip != "" -%>
export OS_SERVICE_ENDPOINT=<%= @keystone_auth_protocol %>://<%= @keystone_ip %>:35357/<%= @keystone_version %>/
<% elsif @internal_vip != "" -%>
export OS_SERVICE_ENDPOINT=<%= @keystone_auth_protocol %>://<%= @internal_vip %>:35358/<%= @keystone_version %>/
<% else -%>
export OS_SERVICE_ENDPOINT=<%= @keystone_auth_protocol %>://<%= @openstack_ip %>:35357/<%= @keystone_version %>/
<% end -%>
